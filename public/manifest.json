    }
  ],
  "screenshots": [
    {
      "src": "/screenshot1.png",
      "sizes": "360x640",
      "type": "image/png"
    },
    {
      "src": "/screenshot2.png",
      "sizes": "360x640",
      "type": "image/png"
    }
  ],
  "display_override": ["window-controls-overlay", "standalone"],
  "shortcuts": [
    {
      "name": "Tableau de bord",
      "short_name": "Dashboard",
      "url": "/adminDashboard",
      "icons": [{ "src": "/icon-192.png", "sizes": "192x192", "type": "image/png" }]
    }
  ],
  "protocol_handlers": [
    {
      "protocol": "web+fsalyda",
      "url": "/?q=%s"
